@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Banner>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
	Layout = null;
	var items = Model.Value<IEnumerable<IPublishedElement>>("banner");
 }
 
 foreach(var item in items) {
	<h3>item.GetPropertyValue("slideTitle")</h3>
	@Umbraco.Field(item,"bodyText")
	}

