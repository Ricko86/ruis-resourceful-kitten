@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Banner>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
	Layout = null;
	var items = Model.Value<IEnumerable<IPublishedElement>>("bannerItems");

        
 foreach(var item in items)
    {
         string title = item.GetPropertyValue<string>("slideTitle");
         <h1 class="hero-title">@title</h1>
    }

}